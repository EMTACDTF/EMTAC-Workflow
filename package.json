{
  "name": "emtac-workflow",
  "version": "1.0.8",
  "description": "Simple Stable EMTAC Offline App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^25.1.8"
  },
  "productName": "EMTAC WORKFLOW",
  "dependencies": {
    "electron-updater": "^6.6.2",
    "electron-log": "^5.1.4"
  },
  "build": {
    "appId": "com.emtac.workflow",
    "productName": "EMTAC WORKFLOW",
    "files": [
      "**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "EMTACDTF",
        "repo": "EMTAC-Workflow",
        "releaseType": "release"
      }
    ]
  }
}